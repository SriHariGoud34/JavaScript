var marks = 75;

if (marks>=91){
    console.log("A Grade")
}
else if (marks>=81 && marks<=90) {
    console.log("B Grade")
}
else if (marks>=71 && marks<=80) {
    console.log("C Grade")
}
else if(marks>=61 && marks<=70) {
    console.log("D Grade")
}
else if(marks>=51 && marks<=60) {
    console.log("just pass E Grade")
}
else {
    console.log("Fail")
}
